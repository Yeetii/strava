{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Information",
            "Function": "Information"
        }
    },
    "extensions": {
        "serviceBus": {
            "maxConcurrentSessions": 1
        }
    },
    "functionTimeout": "00:10:00"
}