{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Warning",
            "Function": "Warning"
        }
    },
    "extensions": {
        "http": {
        "customHeaders": {
            "Access-Control-Expose-Headers": "session"
        },
        "routePrefix": "api"
        }
    }
}